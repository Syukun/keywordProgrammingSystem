package test;
 
 class Solution {
     public void reverseString(char[] s) {
         int len = s.length;
         char[] tmp = new char[len];
         for(int i = Numberliteral ; i < len ; i++){
             tmp[i] = s[len-Numberliteral-i];
         }
         for(int i = Numberliteral ; i < len ; i++){
             s[i] = tmp[i];
         }
     }
 
     public static void Main(String[] args) {
         String sentense;
         String next;
 		sentense.concat(next).compareTo(sentense.toString())
 