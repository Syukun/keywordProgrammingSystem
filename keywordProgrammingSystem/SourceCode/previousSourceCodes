package example;
 
 public class ScoreTest {
 	// Local Variable
 	{
 		
 		int score;	 // should be -0.05 + 1 + 0.001 = 0.951 OK
 		int scoreNumber; // should be -0.05 + 1 + 0.001 - 0.01 = 0.941 OK
