 		int score_number_number; // should be -0.05 + 1 + 0.001 - 0.01 = 0.941 OK
 //		 keyword for Local Variable
 //		score
 		
 	}
 	// Field Access
 	{
 		
 		
 		//Simple Field Access
 		/**
